      "InterventionList": {
        "Intervention": [
          {
            "InterventionType": "Diagnostic Test",
            "InterventionName": "Neurocognitive battery",
            "InterventionDescription": "Cognitive impairment status (SCD, MCI, dementia), HK-MoCA, Clinical Dementia Rating (CDR sum of squares), Geriatric Depression Scale (GDS-15), Neuropsychiatric Index (NPI), Barthel Index, Lawton's IADL, Life-Space Assessment, Mini-Nutrition Assessment (MNA), Quality of Life for Alzheimer's Disease (QoL-AD, patient and carer parts), frailty status (FRAIL scale), handgrip strength, walking speed, exercise status, sleep quality, Charlson comorbidity index (CCI, age-adjusted).\n\nNACC: Story recall, Benson's complex figure copy, colour trail test (black & white), verbal fluency, digit forward and backward span.",
            "InterventionArmGroupLabelList": {
              "InterventionArmGroupLabel": [
                "Alzheimer's dementia",
                "Cognitively normal controls (CNC)",
                "Mild cognitive impairment (MCI)",
                "Subjective cognitive decline (SCD)",
                "Vascular dementia"
              ]
            }
          },
          {
            "InterventionType": "Diagnostic Test",
            "InterventionName": "MRI",
            "InterventionDescription": "MRI: T1, T2, FLAIR, SWI, DTI, fMRI, ASL, MRS, for selected patients",
            "InterventionArmGroupLabelList": {
              "InterventionArmGroupLabel": [
                "Alzheimer's dementia",
                "Cognitively normal controls (CNC)",
                "Mild cognitive impairment (MCI)",
                "Subjective cognitive decline (SCD)",
                "Vascular dementia"
              ]
            }
          },
          {
            "InterventionType": "Biological",
            "InterventionName": "Blood tests",
            "InterventionDescription": "Stored samples (unanalysed): serum, plasma, buffy coat (PBMC); also processed for microvesicles and exosome analysis",
            "InterventionArmGroupLabelList": {
              "InterventionArmGroupLabel": [
                "Alzheimer's dementia",
                "Cognitively normal controls (CNC)",
                "Mild cognitive impairment (MCI)",
