      "InterventionList": {
        "Intervention": [
          {
            "InterventionType": "Biological",
            "InterventionName": "Allogeneic Umbilical Cord Mesenchymal Stem Cell",
            "InterventionDescription": "All research samples will be treated by Intravenous injection of allogeneic mesenchymal stem cells and hemodynamic observation will be done for 24 hours after treated. Periodic post-treatment observation will be done on 1st month, 3rd month, and 6th month after therapy. The Independent variable in this research is Child-Pugh B Cirrhosis caused by Hepatitis B, while the dependent variable is the degree of liver function damage consisting of an examination of liver function, Child-Pugh score, and MELD score.",
            "InterventionArmGroupLabelList": {
              "InterventionArmGroupLabel": [
                "intravenous injection of UC-MSC"
              ]
            }
          }
        ]
      }
