    "OutcomesModule": {
      "PrimaryOutcomeList": {
        "PrimaryOutcome": [
          {
            "PrimaryOutcomeMeasure": "Determine functional differences of peri-prostatic adipose tissue from lean vs obese patients on prostate cancer cell lines",
            "PrimaryOutcomeDescription": "Proliferation, migration, invasion, apoptosis and epthelial-mesenchymal transition assays will be performed using isolated exosomes on prostate cancer cell lines",
            "PrimaryOutcomeTimeFrame": "Through to study completion, on average 3 years"
          },
          {
            "PrimaryOutcomeMeasure": "Identification of exosomal small RNAs transferred between human adipose tissue to prostate cancer cells lines",
            "PrimaryOutcomeDescription": "Exosomal RNA transferred from human adipose tissue to prostate cancer cell lines will be isolated using a tagged magnetic bead isolation method and sequenced to identify transferred small RNAs",
            "PrimaryOutcomeTimeFrame": "Through to study completion, on average 3 years"
          },
          {
            "PrimaryOutcomeMeasure": "Assess how exosomal small RNAs from lean vs obese patients affect cancer regulation",
            "PrimaryOutcomeDescription": "Prostate cancer cell lines will be treated with isolated exosomes, then parental and treated cells sequenced to identify differentially expressed RNA and underlying changes",
            "PrimaryOutcomeTimeFrame": "Through to study completion, on average 3 years"
          },
          {
            "PrimaryOutcomeMeasure": "Attempt to replicate functional changes observed",
            "PrimaryOutcomeDescription": "Treatment of prostate cancer cell lines with microRNA mimics or inhibitors and comparison of changes with exosome treated cell lines",
            "PrimaryOutcomeTimeFrame": "Through to study completion, on average 3 years"
          }
        ]
      }
    },
