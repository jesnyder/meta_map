    "OutcomesModule": {
      "PrimaryOutcomeList": {
        "PrimaryOutcome": [
          {
            "PrimaryOutcomeMeasure": "exosomal PD1 expression",
            "PrimaryOutcomeDescription": "Peripheral blood (≈20 ml) will be collected from non-OSAS or OSAS subjects on EDTA-coated tubes. Exosomes will be isolated by centrifugation. For detecting exosome-associated surface proteins by flow cytometry, exosomes will be first captured on magnetic beads. The bead/exosome complexes will be then co-incubated with the labeled detection antibodies, either anti-PD1. Next, the complexes will be washed 3x resuspended in 300uL of PBS for antigen detection by flow cytometry.",
            "PrimaryOutcomeTimeFrame": "at baseline"
          },
          {
            "PrimaryOutcomeMeasure": "exosomal PD-L1 expression",
            "PrimaryOutcomeDescription": "Peripheral blood (≈20 ml) will be collected from non-OSAS or OSAS subjects on EDTA-coated tubes. Exosomes will be isolated by centrifugation. For detecting exosome-associated surface proteins by flow cytometry, exosomes will be first captured on magnetic beads. The bead/exosome complexes will be then co-incubated with the labeled detection antibodies, either anti-PD-L1. Next, the complexes will be washed 3x resuspended in 300uL of PBS for antigen detection by flow cytometry.",
            "PrimaryOutcomeTimeFrame": "at baseline"
          }
        ]
      }
