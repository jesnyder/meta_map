    "ArmsInterventionsModule": {
      "ArmGroupList": {
        "ArmGroup": [
          {
            "ArmGroupLabel": "Control group",
            "ArmGroupDescription": "Blood samples were collected from 100 control with normal pregnancies. Thirty fresh umbilical cord samples of women with healthy pregnancies (n=15) were retrieved during caesarean deliveries and umibilical cord mesenchymal stem cells (UCMSCs) were isolated from Wharton jelly."
          },
          {
            "ArmGroupLabel": "Preeclampsia group",
            "ArmGroupDescription": "Blood samples were collected from 100 patients with PE. Thirty fresh umbilical cord samples of PE patients (n=15) were retrieved during caesarean deliveries and UCMSCs were isolated from Wharton jelly."
          }
        ]
      }
    },
